From 5995fdb10d3a0c952fcef20d4a112e5fd370fff6 Mon Sep 17 00:00:00 2001
From: Ishimoto Shinobu <nagakamira@gmail.com>
Date: Fri, 26 Feb 2021 00:43:34 +0900
Subject: [PATCH] adjust defaults

Signed-off-by: Ishimoto Shinobu <nagakamira@gmail.com>
---
 schemas/org.cinnamon.desktop.background.gschema.xml.in.in | 2 +-
 schemas/org.cinnamon.desktop.interface.gschema.xml.in.in  | 4 ++--
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/schemas/org.cinnamon.desktop.background.gschema.xml.in.in b/schemas/org.cinnamon.desktop.background.gschema.xml.in.in
index c14228b..0116c52 100644
--- a/schemas/org.cinnamon.desktop.background.gschema.xml.in.in
+++ b/schemas/org.cinnamon.desktop.background.gschema.xml.in.in
@@ -10,7 +10,7 @@
       </_description>
     </key>
     <key name="picture-uri" type="s">
-      <default>'file://@datadir@/themes/Adwaita/backgrounds/adwaita-timed.xml'</default>
+      <default>'file://@datadir@/backgrounds/default.jpg'</default>
       <_summary>Picture URI</_summary>
       <_description>
         URI to use for the background image. Not that the backend only supports
diff --git a/schemas/org.cinnamon.desktop.interface.gschema.xml.in.in b/schemas/org.cinnamon.desktop.interface.gschema.xml.in.in
index d913303..3b2f135 100644
--- a/schemas/org.cinnamon.desktop.interface.gschema.xml.in.in
+++ b/schemas/org.cinnamon.desktop.interface.gschema.xml.in.in
@@ -97,14 +97,14 @@
       </_description>
     </key>
     <key name="icon-theme" type="s">
-      <default>'gnome'</default>
+      <default>'breeze'</default>
       <_summary>Icon Theme</_summary>
       <_description>
         Icon theme to use for the panel, nautilus etc.
       </_description>
     </key>
     <key name="icon-theme-backup" type="s">
-      <default>'gnome'</default>
+      <default>'breeze'</default>
       <_summary>Icon Theme Backup</_summary>
       <_description>
         This is used to store the current icon theme when high contrast is enabled.
