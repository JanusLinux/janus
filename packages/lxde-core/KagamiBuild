# Depends on:  st lxmenu-data menu-cache pcmanfm lxpanel lxappearance lxsession lxde-common
# Section:     metapackages

name=lxde-core
version=0.5.0.20210224
release=1

build() {
	mkdir -p "$PKG"/usr/share/metas/$name
	touch "$PKG"/usr/share/metas/$name/$name.meta
}
