# Depends on:  gnome-core baobab cheese evince file-roller gedit gnome-calculator gnome-calendar gnome-characters gnome-clocks gnome-contacts gnome-disk-utility gnome-font-viewer gnome-initial-setup gnome-logs gnome-maps gnome-photos gnome-screenshot gnome-software gnome-system-monitor gnome-terminal gnome-weather rhythmbox seahorse totem sushi
# Section:     metapackages

name=gnome
version=3.38.0.20201001
release=1

build() {
	mkdir -p "$PKG"/usr/share/metas/$name
	touch "$PKG"/usr/share/metas/$name/$name.meta
}
