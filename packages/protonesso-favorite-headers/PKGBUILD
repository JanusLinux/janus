# Maintainer: protonesso <nagakamira@gmail.com>

pkgname=protonesso-favorite-headers
pkgver=1488
pkgrel=2
pkgdesc="h8 nigs"
arch=('any')
url="https://github.com/protonesso"
license=('BSD')
source=('queue.h'
	'cdefs.h'
	'tree.h')
md5sums=('9a3f0035d89763e26ceee03235fd5d93'
         '5b125c4b64aef6d89766f657709cf0e4'
         '8d0bcd3f4b10dfc5c74d5bb4c0d6722b')

package() {
	cd "$srcdir"
	mkdir -p "$pkgdir"/include/sys
	cp queue.h "$pkgdir"/include/sys
	cp cdefs.h "$pkgdir"/include/sys
	cp tree.h "$pkgdir"/include/sys
}
