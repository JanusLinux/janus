pkgname=protonesso-favorite-headers
pkgver=1488
pkgrel=1
pkgdesc="h8 nigs"
arch=('any')
url="https://github.com/protonesso"
license=('BSD')
source=('queue.h'
	'cdefs.h')
md5sums=('8fb7e67b0c7f79046497d1013ad098f3'
         '4d16263b8dad1c2562e5ecf704fb2bea')

package() {
	cd "$srcdir"
	mkdir -p "$pkgdir"/include/sys
	cp queue.h "$pkgdir"/include/sys
	cp cdefs.h "$pkgdir"/include/sys
}
