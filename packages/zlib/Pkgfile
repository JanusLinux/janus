# Description: A compression/decompression Library
# URL:         http://www.zlib.net/
# Maintainer:  protonesso, nagakamira at gmail dot com

name=zlib
version=1.2.11
release=1
source=(https://zlib.net/$name-$version.tar.xz)

build() {
	cd $name-$version
	./configure \
		--prefix=/usr
	make
	make DESTDIR=$PKG install
	clean-package $PKG
}
