# Description: Specific januslinux scripts
# URL:         https://januslinux.github.io/
# Maintainer:  protonesso, nagakamira at gmail dot com

name=scripts
version=1.0
release=1
source=(genfstab
	recovinit
	zzz)

build() {
	mkdir -p $PKG/usr/bin
	install -m755 $SRC/genfstab $PKG/usr/bin
	install -m755 $SRC/recovinit $PKG/usr/bin
	install -m755 $SRC/zzz $PKG/usr/bin
}
