# Description: A fast, simple LZW file compressor
# URL:         https://github.com/vapier/ncompress
# Maintainer:  protonesso, nagakamira at gmail dot com
# Priority:    required
# Section:     base

name=ncompress
version=4.2.4.6
release=2
options=('bootstrap')
source=("https://github.com/vapier/ncompress/archive/v$version.tar.gz")

build() {
	cd "$SRC"/$name-$version
	make
	make DESTDIR="$PKG" PREFIX=/usr install_core
}
