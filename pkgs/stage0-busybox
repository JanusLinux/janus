. $KEEP/common.sh

PKG=stage0-busybox
VER=1.27.2

tarxf http://busybox.net/downloads/ busybox-$VER .tar.bz2

pkgprep

CROSS_COMPILE=$TARGET-

make ARCH=$KARCH CROSS_COMPILE=$TARGET- defconfig
sed -i "s/.*CONFIG_STATIC.*/CONFIG_STATIC=y/" .config
make ARCH=$KARCH CROSS_COMPILE=$TARGET- -j$JOBS
make ARCH=$KARCH CROSS_COMPILE=$TARGET- CONFIG_PREFIX=$OUT install

packpkg
installhostpkg
